{
  "name": "react-bits",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run jsrepo:build && npm run shadcn:build && npm run llms:text && vite build",
    "new:component": "node scripts/generateComponent.js",
    "shadcn:build": "npm run shadcn:generate && shadcn build",
    "shadcn:generate": "node ./scripts/generateShadcnRegistry.js",
    "llms:text": "node ./scripts/generateLlmsText.js",
    "jsrepo:build": "concurrently -p \"[{name}]\" -n \"default,readmes\" \"npm run jsrepo:default\" \"node ./scripts/generateJsrepoReadmes.js\"",
    "jsrepo:default": "jsrepo build --dirs ./src/content --output-dir ./public/default",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext js,jsx --fix",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.2.4",
    "@chakra-ui/react": "^3.20.0",
    "@emotion/react": "^11.14.0",
    "@gsap/react": "^2.1.2",
    "@react-three/drei": "^10.7.4",
    "@react-three/fiber": "^9.3.0",
    "@react-three/postprocessing": "^3.0.4",
    "@react-three/rapier": "^2.1.0",
    "@tailwindcss/vite": "^4.1.13",
    "@use-gesture/react": "^10.2.27",
    "antd": "^5.27.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "gl-matrix": "^3.4.3",
    "gsap": "^3.13.0",
    "lenis": "^1.3.5",
    "lucide-react": "^0.542.0",
    "maath": "^0.10.8",
    "mathjs": "^14.6.0",
    "matter-js": "^0.20.0",
    "meshline": "^3.3.1",
    "motion": "^12.23.12",
    "next-themes": "^0.4.6",
    "ogl": "^1.0.11",
    "postprocessing": "^6.36.0",
    "react": "^19.0.0",
    "react-confetti": "^6.2.2",
    "react-dom": "^19.0.0",
    "react-haiku": "^2.2.0",
    "react-icons": "^5.5.0",
    "react-router-dom": "^6.26.0",
    "react-syntax-highlighter": "^15.6.1",
    "sonner": "^1.7.1",
    "tailwind-merge": "^3.3.1",
    "three": "^0.167.1"
  },
  "devDependencies": {
    "@types/matter-js": "^0.19.8",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.1.2",
    "eslint": "^8.57.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-react": "^7.34.3",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "eslint-plugin-unused-imports": "^4.1.4",
    "jsrepo": "^1.30.1",
    "postcss": "^8.5.6",
    "postcss-safe-parser": "^7.0.1",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "shadcn": "^3.1.0",
    "tailwindcss": "^4.1.13",
    "tw-animate-css": "^1.3.7",
    "typescript": "^5.7.3",
    "vite": "^5.3.4"
  }
}
